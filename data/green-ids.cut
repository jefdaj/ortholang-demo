syne = concat_faa (gbk_to_faa_each (load_gbk_each ["SynPCC7942_chr.gbk",
                                                   "SynPCC7942_pANL.gbk"]))
chlamy = load_faa "Creinhardtii_281_v5.5.protein.fa"
athal = translate (load_fna "Araport11_genes.201606.cds.fasta")
